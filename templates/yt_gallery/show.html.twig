{% extends 'baseAdmin.html.twig' %}


{% block body %}
    <h1>YtGallery</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ yt_gallery.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ yt_gallery.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ yt_gallery.description }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ yt_gallery.url }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('yt_gallery_index') }}">back to list</a>

    <a href="{{ path('yt_gallery_edit', {'id': yt_gallery.id}) }}">edit</a>

    {{ include('yt_gallery/_delete_form.html.twig') }}
{% endblock %}

    <div class="col-sm-8">
        <h1 class="my-4">{{photo.title|capitalize}}</h1>
        <div><a class="btn btn-sm btn-warning" href="{{path('photo_gallery',{'id':photo.album.id,'slug':photo.album.name|slug})}}"><i class="fa fa-arrow-left"></i> Back to {{photo.album.name}} Album</a></div>
        <img src="{{path('photogallery_thumb',{'id':photo.id,'size':710,'name':photo.fileName})}}" style="width: 100%">


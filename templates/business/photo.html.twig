{% extends "business/base.html.twig" %}
{% block title %}{{photo.title}}{% endblock %}
{% block description %}{{photo.description}}{% endblock %}
{% block opengraph %}
    <meta property="og:title" content="{{photo.title}}" />
    <meta property="og:description" content="{{photo.description}}" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="{{url('photo_gallery_view',{'id':photo.id,'slug':photo.title|slug})}}" />
    <meta property="og:image" content="{{url('photogallery_thumb',{'id':photo.id,'size':710,'name':photo.fileName})}}" />
{% endblock %}

